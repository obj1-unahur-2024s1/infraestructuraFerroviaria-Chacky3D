class Locomotora {
	
	const peso
	const capacidadDeArrastre
	const velocidadMaxima
	
	method esEficiente() { return capacidadDeArrastre >= peso * 5}
	method velocidadMaxima() { return velocidadMaxima }
	method capacidadDeArrastre() { return capacidadDeArrastre }
	method pesoMaximo() { return peso }
	
}
